# This is an array
var names = ["Jim", "Bob"]

# This is a function
function printNames:
	loop names as name:
		print name

# Print names
printNames names

# Print first name
function firstName array:
	return array[0]

var name = firstName names
if name = "Bob":
	print "First name is Bob"

